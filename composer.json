{
    "name": "wu/giorgio-socket",
    "description": "laravel swoole socket server",
    "keywords": [
        "laravel",
        "swoole",
        "socket",
        "im"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "m-finder",
            "email": "m@m-finder.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "ext-swoole": "*",
        "laravel/framework": ">=9.0"
    },
    "autoload": {
        "psr-4": {
            "GiorgioSocket\\": "src",
            "GiorGioSocket\\Services\\Handlers\\": "src/Services/Handlers"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GiorgioSocket\\Providers\\SocketServiceProvider",
                "GiorgioSocket\\Providers\\EventServiceProvider"
            ]
        }
    }
}
